<!-- Tutorial_03_Initialize (JavaScript) -->
<Module>
	<!-- 
	* STEP 1
	* - Rename our application
	* - Add "wave" as a required feature to ModulePrefs
	-->
	<ModulePrefs title="Tutorial_03_Initialize" height="250">
        <Require feature="wave"/>
    </ModulePrefs>
	<Content type="html">
		<![CDATA[
			<!--
			 * STEP 2
			 * Add Javascript that sets callbacks and renders the gadget.
			 * 
			 * STEP 3
			 * Initialize our gadget by passing init().
			-->
			<script type="text/javascript" src="https://www.googledrive.com/host/0B0DPCg3D7fj6WlRxV2UxODlqZDA"></script>
			
			<!-- Add some basic HTML structure to our gadget-->
			<div id="app">
				<div id="header"><h2>OpenSocial Tutorial Tool</h2></div>
				<div id="body"></div>
				<div id="footer"></div>
			</div>
			
			<!-- Add CSS styling to set properties for app, header, body and footer elements. -->
			<link rel="stylesheet" type="text/css" href="https://www.googledrive.com/host/0B0DPCg3D7fj6MGtuYUszMkEwM3c">
		]]>
	</Content>
</Module>